<PropertyList include="../IDG-MD-11X/MD-11-main.xml">

	<sim>
		
		<author>it0uchpods Design Group: Joshua Davidson (it0uchpods), Jonathan Redpath (legoboyvdlp), Matthew Anderson (FB), J Maverick 16</author>
		
		<checklists include="MD-11-Checklists.xml"/>
		
		<previews>
			<preview>
				<type>exterior</type>
				<splash type="bool">true</splash>
				<path>Splash/splash1.png</path>
			</preview>
		</previews>

		<startup>
			<splash-texture>Aircraft/IDG-MD-10X/Splash/SplashOld.png</splash-texture>
		</startup>
			
		<view>
			<name>Captain View</name>
			<internal type="bool" archive="y">true</internal>
			<config>
				<x-offset-m archive="y">-0.5238</x-offset-m>
				<y-offset-m archive="y">7.2709</y-offset-m>
				<z-offset-m archive="y">-24.40</z-offset-m>
				<default-field-of-view-deg type="double">80</default-field-of-view-deg>
				<pitch-offset-deg archive="y">-15.0</pitch-offset-deg>
			</config>
		</view>
		<view n="100">
			<name>First Officer View</name>
			<type>lookfrom</type>
			<enabled type="bool">true</enabled>
			<internal type="bool" archive="y">true</internal>
			<config>
				<from-model type="bool">true</from-model>
				<x-offset-m archive="y">0.5238</x-offset-m>
				<y-offset-m archive="y">7.2709</y-offset-m>
				<z-offset-m archive="y">-24.4</z-offset-m>
				<default-field-of-view-deg type="double">80</default-field-of-view-deg>
				<pitch-offset-deg archive="y">-15.0</pitch-offset-deg>
			</config>
		</view>
		<view n="101"> 
			<name>Overhead Panel</name>
			<type>lookfrom</type>
			<enabled type="bool">true</enabled>
			<internal type="bool" archive="y">true</internal>
			<config>
				<from-model type="bool">true</from-model>
				<x-offset-m archive="y">0.0</x-offset-m>
				<y-offset-m archive="y">7.2709</y-offset-m>
				<z-offset-m archive="y">-24.4</z-offset-m>
				<pitch-offset-deg archive="y">52.0</pitch-offset-deg>
				<default-field-of-view-deg type="double">100</default-field-of-view-deg>
			</config>
		</view>
		<view n="102">
			<name>Forward Pedestal View</name>
			<type>lookfrom</type>
			<internal archive="y">true</internal>
			<config>
				<from-model type="bool">true</from-model>
				<x-offset-m archive="y">0.0</x-offset-m>
				<y-offset-m archive="y">7.3209</y-offset-m>
				<z-offset-m archive="y">-24.6</z-offset-m>
				<pitch-offset-deg archive="y">-68</pitch-offset-deg>
				<default-field-of-view-deg type="double">77</default-field-of-view-deg>
			</config>
		</view>
		<view n="103">
			<name>Aft Pedestal View</name>
			<type>lookfrom</type>
			<internal archive="y">true</internal>
			<config>
				<from-model type="bool">true</from-model>
				<x-offset-m archive="y">0.0</x-offset-m>
				<y-offset-m archive="y">7.2709</y-offset-m>
				<z-offset-m archive="y">-24.44</z-offset-m>
				<pitch-offset-deg archive="y">-90</pitch-offset-deg>
				<default-field-of-view-deg type="double">73</default-field-of-view-deg>
			</config>
		</view>
		<view n="104">
			<name>AFS Panel View</name>
			<type>lookfrom</type>
			<internal archive="y">true</internal>
			<config>
				<from-model type="bool">true</from-model>
				<x-offset-m archive="y">0.0</x-offset-m>
				<y-offset-m archive="y">7.2709</y-offset-m>
				<z-offset-m archive="y">-24.4</z-offset-m>
				<pitch-offset-deg archive="y">-14.6</pitch-offset-deg>
				<default-field-of-view-deg type="double">63</default-field-of-view-deg>
			</config>
		</view>
		<view n="105">
			<name>Left Wing View</name>
			<type>lookfrom</type>
			<enabled type="bool">true</enabled>
			<internal type="bool" archive="y">true</internal>
			<config>
				<from-model type="bool">true</from-model>
				<x-offset-m archive="y">-3.53</x-offset-m>
				<y-offset-m archive="y">6.96</y-offset-m>
				<z-offset-m archive="y">5.69</z-offset-m>
				<default-field-of-view-deg type="double">110.2</default-field-of-view-deg>
				<heading-offset-deg archive="y">100</heading-offset-deg>
				<pitch-offset-deg archive="y">-7</pitch-offset-deg>
			</config>
		</view>
		<view n="106">
			<name>Right Wing View</name>
			<type>lookfrom</type>
			<enabled type="bool">true</enabled>
			<internal type="bool" archive="y">true</internal>
			<config>
				<from-model type="bool">true</from-model>
				<x-offset-m archive="y">3.53</x-offset-m>
				<y-offset-m archive="y">6.96</y-offset-m>
				<z-offset-m archive="y">5.69</z-offset-m>
				<default-field-of-view-deg type="double">110.2</default-field-of-view-deg>
				<heading-offset-deg archive="y">260</heading-offset-deg>
				<pitch-offset-deg archive="y">-7</pitch-offset-deg>
			</config>
		</view>
		
		<menubar>
			<default>
				<menu n="100">
					<label>IDG-MD-10X</label>
				</menu>
			</default>
		</menubar>
		
    </sim>
	
	<consumables n="0">
		<fuel>
			<tank n="0"><name>Left Wing</name></tank>
			<tank n="1"><name>Center</name></tank>
			<tank n="2"><name>Right Wing</name></tank>
			<tank n="3"><name>Auxiliary</name></tank>
			<tank n="4"><name>INOP</name></tank>
			<tank n="5"><name>Engine 1 Pipes</name></tank>
			<tank n="6"><name>Engine 2 Pipes</name></tank>
			<tank n="7"><name>Engine 3 Pipes</name></tank>
		</fuel>
	</consumables>
	
	<controls n="0">
		<engines n="0">
			<engine n="3">
				<n1 alias="/options/boom-pos-norm"/> <!-- Hijack the N1 property of engine 3 to send boom position over MP -->
			</engine>
		</engines>
		<hydraulic n="0">
			<aileron-droop-enable type="bool">0</aileron-droop-enable>
		</hydraulic>
	</controls>
	
	<lsas n="0">
		<enable type="bool">0</enable>
	</lsas>
	
	<nasal n="0">
		<addonhelper>
			<file>Aircraft/IDG-MD-10X/AddonHelper/addonhelper.nas</file>
		</addonhelper>
	</nasal>

</PropertyList>
